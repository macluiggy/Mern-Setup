#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/nodemon@2.0.14/node_modules/nodemon/bin/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/nodemon@2.0.14/node_modules/nodemon/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/nodemon@2.0.14/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/node_modules:/home/macluiggy/Documents/PROGRAMMING/node_modules:/home/macluiggy/Documents/node_modules:/home/macluiggy/node_modules:/home/node_modules:/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/nodemon/bin/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/nodemon/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/nodemon@2.0.14/node_modules/nodemon/bin/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/nodemon@2.0.14/node_modules/nodemon/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/nodemon@2.0.14/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/node_modules:/home/macluiggy/Documents/PROGRAMMING/node_modules:/home/macluiggy/Documents/node_modules:/home/macluiggy/node_modules:/home/node_modules:/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/nodemon/bin/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/nodemon/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../nodemon/bin/nodemon.js" "$@"
else
  exec node  "$basedir/../nodemon/bin/nodemon.js" "$@"
fi
