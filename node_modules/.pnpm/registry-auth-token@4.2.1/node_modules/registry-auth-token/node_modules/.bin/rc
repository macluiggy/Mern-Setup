#!/bin/sh
basedir=$(dirname "$(echo "$0" | sed -e 's,\\,/,g')")

case `uname` in
    *CYGWIN*) basedir=`cygpath -w "$basedir"`;;
esac

if [ -z "$NODE_PATH" ]; then
  export NODE_PATH="/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/rc@1.2.8/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/node_modules:/home/macluiggy/Documents/PROGRAMMING/node_modules:/home/macluiggy/Documents/node_modules:/home/macluiggy/node_modules:/home/node_modules:/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules/registry-auth-token/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules"
else
  export NODE_PATH="$NODE_PATH:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/rc@1.2.8/node_modules/rc/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/rc@1.2.8/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/node_modules:/home/macluiggy/Documents/PROGRAMMING/node_modules:/home/macluiggy/Documents/node_modules:/home/macluiggy/node_modules:/home/node_modules:/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules/registry-auth-token/node_modules:/home/macluiggy/Documents/PROGRAMMING/MERN/MERN-SETUP-VERCEL/node_modules/.pnpm/registry-auth-token@4.2.1/node_modules"
fi
if [ -x "$basedir/node" ]; then
  exec "$basedir/node"  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
else
  exec node  "$basedir/../../../../../rc@1.2.8/node_modules/rc/cli.js" "$@"
fi
